<?xml version = '1.0' encoding = 'UTF-8'?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd"
         version="3.0">
  <module-name>RunScript</module-name>
  <display-name>Run Script Module</display-name>
  <description>Run Script Services</description>
  <session-config>
    <tracking-mode>URL</tracking-mode>
  </session-config>
  <servlet>
    <display-name>Script Servlet</display-name>
    <servlet-name>scriptservlet</servlet-name>
    <servlet-class>nl.jacbeekers.runscript.RunScriptServlet</servlet-class>
    <load-on-startup>1</load-on-startup>
    <enabled>true</enabled>
  </servlet>
  <servlet-mapping>
    <servlet-name>scriptservlet</servlet-name>
    <url-pattern>runscript/*</url-pattern>
  </servlet-mapping>
</web-app>
